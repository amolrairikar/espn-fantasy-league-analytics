{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run Module (Prompt)",
      "type": "debugpy",
      "request": "launch",
      "module": "${input:moduleName}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/lambda_layer:${env:PYTHONPATH}"
      }
    }
  ],
  "inputs": [
    {
      "id": "moduleName",
      "type": "promptString",
      "description": "Enter module to run (e.g. api.file_I_am_running)"
    }
  ]
}
